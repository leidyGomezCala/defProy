    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div class="example-wrapper">

    <div class="row">
        <div class="input-group mb-3">
            <a href="javascript:history.back()" class="btn btn-warning"> <img src="https://img.icons8.com/material-outlined/24/000000/back--v1.png"/>Atr√°s</a>
        </div>
    </div>
    <h1>Registro</h1>
    {% for message in app.flashes('exito') %}
